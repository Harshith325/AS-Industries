                <h4>{{name}}</h4>
                <p>{{price}}</p>
                <div class="portfolio-links">
                  <a href="{{img}}" data-gallery="portfolioGallery" class="portfolio-lightbox" title="p1"><i class="ri-zoom-in-line"></i></a>
                  <a href="/products" title="More Details"><i class="ri-links-fill"></i></a>
                </div>
              </div>
            </div>
